<dataSourceProvider xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <dataSourceName>Shine-DS</dataSourceName>
    <serviceId>Shine</serviceId>
    <!--Optional:-->
    <configProperties>
        <!--Zero or more repetitions:-->
        <configProperty>
            <name>HostName</name>
            <type>STRING</type>
            <value></value>
            <isRequired>true</isRequired>
            <propValues></propValues>
        </configProperty>
        <configProperty>
            <name>Port</name>
            <type>INTEGER</type>
            <value></value>
            <isRequired>true</isRequired>
            <propValues></propValues>
        </configProperty>
        <configProperty>
            <name>Protocol</name>
            <type>ENUMERATION</type>
            <value></value>
            <isRequired>true</isRequired>
            <propValues>http</propValues>
            <propValues>https</propValues>
        </configProperty>
        <configProperty>
            <name>AuthPolicy</name>
            <type>ENUMERATION</type>
            <value></value>
            <isRequired>false</isRequired>
            <propValues>lwsso</propValues>
			<propValues>basic</propValues>
        </configProperty>
    </configProperties>
    <!--Optional:-->
    <testProperties>
        <!--Zero or more repetitions:-->
        <configProperty>
            <name>string</name>
            <type>string</type>
            <value></value>
            <isRequired>true</isRequired>
            <propValues>string</propValues>
        </configProperty>
    </testProperties>
</dataSourceProvider>